pipeline {
    agent any 
    stages {
        stage('Verify Branch'){
            steps{
             echo "$GIT_BRANCH"
            }
        }
        stage('Docker build'){
            steps{
            sh 'docker images -a'
            sh """
            cd azure-voting-app-redis/azure-vote/
            docker  build -t jenkins-pipeline
            docker images -a
            """
             }
        }
    }

}
